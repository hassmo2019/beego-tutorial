{
  "AWSEBDockerrunVersion": "2",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "hassmo/beego-tutorial",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-region": "us-east-2",
          "awslogs-group": "/aws/elasticbeanstalk/beego-tutorial-ogido"
        }
      }
    }
  ]
}
